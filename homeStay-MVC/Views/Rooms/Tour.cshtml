
@model IEnumerable<homeStay_MVC.Models.Admin>

@{
    ViewBag.Title = "Tour";
}
<link rel="stylesheet" href="https://app.staticsave.com/root/root.css">
<link href="~/Content/Index.css" rel="stylesheet" />

<style>
    #cmp {
        display: block;
        margin-top: 50px;
        margin-bottom: 10px;
        margin-right: 10%;
        border: 2px solid rgb(204, 238, 255);
        border-radius:20px;
    }
    .imm {
        height: 200px;
        width: 300px;
    }
</style>

<div id="cmp">
    <p href="#" style="text-align:center">as</p>
    <div id="te">
        @Html.Partial("_TourBook")
    </div>
</div>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script>
        $(document).ready(function () {
            $("#star5").click(function () {
                $("#Str5").slideDown("slow");
                $("#Str4").slideUp("slow");
                $("#Str3").slideUp("slow");

                $("#star5").css("border-radius", "50px");
                $("#star5").css("filter", "brightness(1.3)");
                $("#star4").css("border-radius", "10px");
                $("#star4").css("filter", "brightness(1)");
                $("#star3").css("border-radius", "10px");
                $("#star3").css("filter", "brightness(1)");
            });
            $("#star4").click(function () {
                $("#Str4").slideDown("slow");
                $("#Str5").slideUp("slow");
                $("#Str3").slideUp("slow");

                $("#star4").css("border-radius", "50px");
                $("#star4").css("filter", "brightness(1.3)");
                $("#star5").css("border-radius", "10px");
                $("#star5").css("filter", "brightness(1)");
                $("#star3").css("border-radius", "10px");
                $("#star3").css("filter", "brightness(1)");
            });
            $("#star3").click(function () {
                $("#Str3").slideDown("slow");
                $("#Str4").slideUp("slow");
                $("#Str5").slideUp("slow");

                $("#star3").css("border-radius", "50px");
                $("#star3").css("filter", "brightness(1.3)");
                $("#star5").css("border-radius", "10px");
                $("#star5").css("filter", "brightness(1)");
                $("#star4").css("border-radius", "10px");
                $("#star4").css("filter", "brightness(1)");

            });
        });

        $(document).ready(function () {
            $("#pri1").click(function () {
                $("#Pr1").slideDown("slow");
                $("#Pr2").slideUp("slow");

                $("#pri1").css("border-radius", "50px");
                $("#pri1").css("filter", "brightness(1.3)");
                $("#pri2").css("border-radius", "10px");
                $("#pri2").css("filter", "brightness(1)");
            });
            $("#pri2").click(function () {
                $("#Pr2").slideDown("slow");
                $("#Pr1").slideUp("slow");

                $("#pri2").css("border-radius", "50px");
                $("#pri2").css("filter", "brightness(1.3)");
                $("#pri1").css("border-radius", "10px");
                $("#pri1").css("filter", "brightness(1)");
            });
        });

        $(document).ready(function () {
            $("#sWstar").click(function () {
                $("#sss").slideDown("slow");
                $("#aaa").slideUp("slow");
                $("#ppp").slideUp("slow");

                $("#sWstar").css("border-radius", "50px");
                $("#sWstar").css("filter", "brightness(1.3)");
                $("#sWprice").css("border-radius", "10px");
                $("#sWprice").css("filter", "brightness(1)");
                $("#sWall").css("border-radius", "10px");
                $("#sWall").css("filter", "brightness(1)");
            });
            $("#sWprice").click(function () {
                $("#ppp").slideDown("slow");
                $("#aaa").slideUp("slow");
                $("#sss").slideUp("slow");

                $("#sWprice").css("border-radius", "50px");
                $("#sWprice").css("filter", "brightness(1.3)");
                $("#sWstar").css("border-radius", "10px");
                $("#sWstar").css("filter", "brightness(1)");
                $("#sWall").css("border-radius", "10px");
                $("#sWall").css("filter", "brightness(1)");
            });
            $("#sWall").click(function () {
                $("#aaa").slideDown("slow");
                $("#sss").slideUp("slow");
                $("#ppp").slideUp("slow");

                $("#sWall").css("border-radius", "50px");
                $("#sWall").css("filter", "brightness(1.3)");
                $("#sWprice").css("border-radius", "10px");
                $("#sWprice").css("filter", "brightness(1)");
                $("#sWstar").css("border-radius", "10px");
                $("#sWstar").css("filter", "brightness(1)");
            });
        });
    </script>
